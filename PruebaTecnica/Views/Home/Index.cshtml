@model List<PruebaTecnica.Models.Noticia>
@{
    ViewData["Title"] = "Home Page";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Content</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var noticia in Model)
        {
            <tr>
                <td>@noticia.Titulo</td>
                <td>@noticia.Descripcion</td>
            </tr>
        }
    </tbody>
